{
    "name": "20minutes/php-draftjs-html",
    "description": "Export Draft.js ContentState to HTML",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Will Taylor-Jackson",
            "email": "will.taylorjackson@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">= 7.2",
        "ext-xml": "*",
        "prezly/draft-php": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/phpunit-bridge": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Willtj\\PhpDraftjsHtml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Willtj\\PhpDraftjsHtml\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
